msgid " (invalid encoding)"
msgstr "(தவறான குறியாக்கம்)"

msgid "%.1f GB"
msgstr "%.1F சிபி"

msgid "%.1f KB"
msgstr "%.1f KB"

msgid "%.1f MB"
msgstr "%.1f Mb"

msgid "%s filetype"
msgstr "%S FILETYPE"

msgid "%s type"
msgstr "%s வகை"

msgid ""
"'%s' is not a valid character following a '<' character; it may not begin an"
" element name"
msgstr ""
"'%s' என்பது ஒரு '<' கதாபாத்திரத்தைத் தொடர்ந்து சரியான பாத்திரம் அல்ல; இது "
"ஒரு உறுப்பு பெயரைத் தொடங்கக்கூடாது"

msgid ""
"'%s' is not a valid character following the characters '</'; '%s' may not "
"begin an element name"
msgstr ""
"'%s' என்பது கதாபாத்திரங்களின் '</' என்ற கதாபாத்திரங்களைப் பின்பற்றும் சரியான"
" பாத்திரம் அல்ல; '%s' ஒரு உறுப்பு பெயரைத் தொடங்கக்கூடாது"

msgid ""
"'%s' is not a valid character following the close element name '%s'; the "
"allowed character is '>'"
msgstr "the allowed character is '>'"

msgid "'%s' is not a valid name "
msgstr "'%s' என்பது சரியான பெயர் அல்ல"

msgid "'%s' is not a valid name: '%c' "
msgstr "'%s' என்பது சரியான பெயர் அல்ல: '%சி'"

msgid "(?R or (?[+-]digits must be followed by )"
msgstr "(? R அல்லது (? [+-] இலக்கங்களை பின்பற்ற வேண்டும்)"

msgid ") without opening ("
msgstr ") திறக்காமல் ("

msgid "A bookmark for URI '%s' already exists"
msgstr "யூரி '%s' க்கான ஒரு புக்மார்க்கு ஏற்கனவே உள்ளது"

msgid "Abstract unix domain socket addresses not supported on this system"
msgstr ""
"சுருக்கம் யூனிக்ச் டொமைன் சாக்கெட் முகவரிகள் இந்த அமைப்பில் "
"ஆதரிக்கப்படவில்லை"

msgid "Added socket is closed"
msgstr "சேர்க்கப்பட்ட சாக்கெட் மூடப்பட்டுள்ளது"

msgid "Application Options:"
msgstr "பயன்பாட்டு விருப்பங்கள்:"

msgid "Association creation not supported on win32"
msgstr "சங்க உருவாக்கம் WIN32 இல் ஆதரிக்கப்படவில்லை"

msgid "Attribute '%s' of element '%s' not found"
msgstr "உறுப்பு '%s' இன் '%s' என்ற பண்புக்கூறு காணப்படவில்லை"

msgid "Attribute value must be non-NULL"
msgstr "பண்புக்கூறு மதிப்பு பூசப்படாததாக இருக்க வேண்டும்"

msgid "Backup file creation failed"
msgstr "காப்பு கோப்பு உருவாக்கம் தோல்வியடைந்தது"

msgid "Can't copy directory over directory"
msgstr "கோப்பகத்தை கோப்பகத்தை நகலெடுக்க முடியாது"

msgid "Can't copy over directory"
msgstr "கோப்பகத்தை நகலெடுக்க முடியாது"

msgid "Can't copy special file"
msgstr "சிறப்பு கோப்பை நகலெடுக்க முடியாது"

msgid "Can't create user MIME configuration folder %s: %s"
msgstr "பயனர் மைம் உள்ளமைவு கோப்புறை %s: %s ஐ உருவாக்க முடியாது"

msgid "Can't create user application configuration folder %s: %s"
msgstr "பயனர் பயன்பாட்டு உள்ளமைவு கோப்புறை %s: %s ஐ உருவாக்க முடியாது"

msgid "Can't create user desktop file %s"
msgstr "பயனர் டெச்க்டாப் கோப்பு %s ஐ உருவாக்க முடியாது"

msgid "Can't do a raw read in g_io_channel_read_line_string"
msgstr "G_IO_CHANNEL_READ_LINE_STRING இல் ஒரு மூல வாசிப்பை செய்ய முடியாது"

msgid "Can't do a raw read in g_io_channel_read_to_end"
msgstr "G_io_channel_read_to_end இல் ஒரு மூல வாசிப்பைச் செய்ய முடியாது"

msgid "Can't find application"
msgstr "பயன்பாட்டைக் கண்டுபிடிக்க முடியவில்லை"

msgid "Can't handle the supplied version the icon encoding"
msgstr "வழங்கப்பட்ட பதிப்பை படவுரு குறியாக்கத்தை கையாள முடியாது"

msgid "Can't handle version %d of GEmblem encoding"
msgstr "செம்லெம் குறியாக்கத்தின் பதிப்பு %d ஐ கையாள முடியாது"

msgid "Can't handle version %d of GEmblemedIcon encoding"
msgstr "செம்லாம்ப்ளிம்பிளிகான் குறியாக்கத்தின் பதிப்பு %d ஐ கையாள முடியாது"

msgid "Can't handle version %d of GFileIcon encoding"
msgstr "Gfileicon குறியாக்கத்தின் பதிப்பு %d ஐ கையாள முடியாது"

msgid "Can't handle version %d of GThemedIcon encoding"
msgstr "Gthemedicon குறியாக்கத்தின் பதிப்பு %d ஐ கையாள முடியாது"

msgid "Can't move directory over directory"
msgstr "கோப்பகத்தின் மீது கோப்பகத்தை நகர்த்த முடியாது"

msgid "Can't open directory"
msgstr "கோப்பகத்தைத் திறக்க முடியாது"

msgid "Can't recursively copy directory"
msgstr "கோப்பகத்தை மீண்டும் மீண்டும் நகலெடுக்க முடியாது"

msgid "Can't rename file, filename already exist"
msgstr "கோப்பை மறுபெயரிட முடியாது, கோப்பு பெயர் ஏற்கனவே உள்ளது"

msgid "Can't rename root directory"
msgstr "ரூட் கோப்பகத்தை மறுபெயரிட முடியாது"

msgid "Cancellable initialization not supported"
msgstr "ரத்து செய்யக்கூடிய துவக்கம் ஆதரிக்கப்படவில்லை"

msgid "Cannot convert fallback '%s' to codeset '%s'"
msgstr "குறைவடையும் '%s' கோட்செட் '%s' ஆக மாற்ற முடியாது"

msgid "Cannot parse double value '%s' for %s"
msgstr "%S க்கு இரட்டை மதிப்பை ' %s' அலச முடியாது"

msgid "Cannot parse integer value '%s' for %s"
msgstr "%S க்கு முழு எண் மதிப்பை ' %s' அலச முடியாது"

msgid "Cannot set permissions on symlinks"
msgstr "சிம்லிங்க்களில் அனுமதிகளை அமைக்க முடியாது"

msgid "Cannot truncate GMemoryInputStream"
msgstr "GmemoryInputstream ஐ துண்டிக்க முடியாது"

msgid "Channel terminates in a partial character"
msgstr "சேனல் ஒரு பகுதி எழுத்தில் முடிவடைகிறது"

msgid "Character out of range for UTF-16"
msgstr "யுடிஎஃப் -16 க்கான தன்மை வரம்பற்றது"

msgid "Character out of range for UTF-8"
msgstr "யுடிஎஃப் -8 க்கான வரம்பிற்கு வெளியே"

msgid "Character reference '%-.*s' does not encode a permitted character"
msgstr "எழுத்து குறிப்பு '%-.*கள்' அனுமதிக்கப்பட்ட தன்மையை குறியாக்கவில்லை"

msgid ""
"Character reference did not end with a semicolon; most likely you used an "
"ampersand character without intending to start an entity - escape ampersand "
"as &amp;"
msgstr ""
"எழுத்து குறிப்பு அரைக்காற்புள்ளியுடன் முடிவடையவில்லை; பெரும்பாலும் நீங்கள் "
"ஒரு நிறுவனத்தைத் தொடங்க விரும்பாமல் ஒரு ஆம்பர்சண்ட் கதாபாத்திரத்தைப் "
"பயன்படுத்தினீர்கள் - ஆம்பர்சண்ட் தப்பிக்க & ஆம்ப்;"

msgid "Connection in progress"
msgstr "இணைப்பு முன்னேற்றத்தில் உள்ளது"

msgid "Containing mount does not exist"
msgstr "மவுண்ட் கொண்டிருப்பது இல்லை"

msgid "Conversion from character set '%s' to '%s' is not supported"
msgstr ""
"எழுத்துக்குறி தொகுப்பிலிருந்து '%s' இலிருந்து '%s' ஆக மாற்றுவது "
"ஆதரிக்கப்படவில்லை"

msgid "Could not allocate %lu bytes to read file \"%s\""
msgstr "\" %s\" கோப்பைப் படிக்க %லு பைட்டுகளை ஒதுக்க முடியவில்லை"

msgid "Could not open converter from '%s' to '%s'"
msgstr "'%S' முதல் '%s' வரை மாற்றி திறக்க முடியவில்லை"

msgid "Could not open converter from '%s' to '%s': %s"
msgstr "'%S' முதல் '%s' வரை மாற்றி திறக்க முடியவில்லை:%s"

msgid "Custom definition for %s"
msgstr "%S க்கான தனிப்பயன் வரையறை"

msgid "DEFINE group contains more than one branch"
msgstr "வரையறுக்கும் குழுவில் ஒன்றுக்கு மேற்பட்ட கிளைகள் உள்ளன"

msgid "Desktop file didn't specify Exec field"
msgstr "டெச்க்டாப் கோப்பு EXEC புலத்தைக் குறிப்பிடவில்லை"

msgid ""
"Document ended unexpectedly after the equals sign following an attribute "
"name; no attribute value"
msgstr ""
"ஒரு பண்புக்கூறு பெயரைத் தொடர்ந்து சமமான அடையாளத்திற்குப் பிறகு ஆவணம் "
"எதிர்பாராத விதமாக முடிந்தது; பண்புக்கூறு மதிப்பு இல்லை"

msgid "Document ended unexpectedly inside a comment or processing instruction"
msgstr ""
"ஒரு கருத்து அல்லது செயலாக்க அறிவுறுத்தலுக்குள் ஆவணம் எதிர்பாராத விதமாக "
"முடிந்தது"

msgid "Document ended unexpectedly inside an attribute name"
msgstr "ஒரு பண்புக்கூறு பெயருக்குள் ஆவணம் எதிர்பாராத விதமாக முடிந்தது"

msgid "Document ended unexpectedly inside an element name"
msgstr "ஆவணம் எதிர்பாராத விதமாக ஒரு உறுப்பு பெயருக்குள் முடிந்தது"

msgid "Document ended unexpectedly inside an element-opening tag."
msgstr ""
"ஒரு உறுப்பு திறக்கும் குறிச்சொல்லின் உள்ளே ஆவணம் எதிர்பாராத விதமாக "
"முடிந்தது."

msgid "Document ended unexpectedly inside the close tag for element '%s'"
msgstr ""
"'%s' என்ற உறுப்புக்கான நெருக்கமான குறிச்சொல்லுக்குள் ஆவணம் எதிர்பாராத விதமாக"
" முடிந்தது"

msgid "Document ended unexpectedly just after an open angle bracket '<'"
msgstr "ஆவணம் எதிர்பாராத விதமாக ஒரு திறந்த கோண அடைப்புக்குப் பிறகு '<'"

msgid "Document ended unexpectedly while inside an attribute value"
msgstr ""
"ஒரு பண்புக்கூறு மதிப்புக்குள் இருக்கும்போது ஆவணம் எதிர்பாராத விதமாக "
"முடிந்தது"

msgid ""
"Document ended unexpectedly with elements still open - '%s' was the last "
"element opened"
msgstr ""
"இன்னும் திறந்த உறுப்புகளுடன் ஆவணம் எதிர்பாராத விதமாக முடிந்தது - '%s' என்பது"
" கடைசி உறுப்பு திறக்கப்பட்டது"

msgid ""
"Document ended unexpectedly, expected to see a close angle bracket ending "
"the tag <%s/>"
msgstr "%s/>"

msgid "Document must begin with an element (e.g. <book>)"
msgstr "ஆவணம் ஒரு உறுப்புடன் தொடங்க வேண்டும் (எ.கா. <BOOK>)"

msgid "Document was empty or contained only whitespace"
msgstr "ஆவணம் காலியாக இருந்தது அல்லது இடைவெளி மட்டுமே இருந்தது"

msgid "Double value '%s' for %s out of range"
msgstr "%s வரம்பிற்கு வெளியே இரட்டை மதிப்பு ' %s'"

msgid "Element '%s' was closed, but the currently open element is '%s'"
msgstr "உறுப்பு '%s' மூடப்பட்டது, ஆனால் தற்போது திறந்த உறுப்பு '%s'"

msgid "Element '%s' was closed, no element is currently open"
msgstr "உறுப்பு '%s' மூடப்பட்டது, தற்போது எந்த உறுப்புகளும் திறக்கப்படவில்லை"

msgid ""
"Empty entity '&;' seen; valid entities are: &amp; &quot; &lt; &gt; &apos;"
msgstr ""
"வெற்று நிறுவனம் '&;' பார்த்தது; செல்லுபடியாகும் நிறுவனங்கள்: & ஆம்ப்; & "
"quot; & lt; & gt; & apos;"

msgid ""
"Entity did not end with a semicolon; most likely you used an ampersand "
"character without intending to start an entity - escape ampersand as &amp;"
msgstr ""
"நிறுவனம் அரைக்காற்புள்ளியுடன் முடிவடையவில்லை; பெரும்பாலும் நீங்கள் ஒரு "
"நிறுவனத்தைத் தொடங்க விரும்பாமல் ஒரு ஆம்பர்சண்ட் கதாபாத்திரத்தைப் "
"பயன்படுத்தினீர்கள் - ஆம்பர்சண்ட் தப்பிக்க & ஆம்ப்;"

msgid "Entity name '%-.*s' is not known"
msgstr "நிறுவனத்தின் பெயர் '%-.*கள்' தெரியவில்லை"

msgid "Enumerator is closed"
msgstr "கணக்கீட்டாளர் மூடப்பட்டுள்ளது"

msgid "Error accepting connection: %s"
msgstr "இணைப்பை ஏற்றுக்கொள்வதில் பிழை: %s"

msgid "Error binding to address: %s"
msgstr "முகவரிக்கு பிழை பிணைப்பு: %s"

msgid "Error closing file: %s"
msgstr "பிழை மூடல் கோப்பு: %s"

msgid "Error closing socket: %s"
msgstr "பிழை மூடும் சாக்கெட்: %s"

msgid "Error closing unix: %s"
msgstr "பிழை மூடல் யூனிக்ச்: %s"

msgid "Error connecting: "
msgstr "இணைக்கும் பிழை:"

msgid "Error connecting: %s"
msgstr "இணைக்கும் பிழை: %s"

msgid "Error creating backup copy: %s"
msgstr "காப்புப்பிரதி நகலை உருவாக்குவது பிழை: %s"

msgid "Error creating directory: %s"
msgstr "கோப்பகத்தை உருவாக்கும் பிழை: %s"

msgid "Error during conversion: %s"
msgstr "மாற்றத்தின் போது பிழை: %s"

msgid "Error getting filesystem info: %s"
msgstr "கோப்பு முறைமை தகவல்களைப் பெறுவதில் பிழை: %s"

msgid "Error launching application: %s"
msgstr "பயன்பாட்டை தொடங்குவதில் பிழை: %s"

msgid "Error making symbolic link: %s"
msgstr "குறியீட்டு இணைப்பை உருவாக்கும் பிழை: %s"

msgid "Error moving file: %s"
